generator client {
  provider = "prisma-client"
  output   = "../generated"
  moduleFormat = "esm"
  runtime = "bun"
}

datasource db {
  provider = "postgresql"
}




model SummarizedText {
  id String @id @map("_id")
  parts        Json[]
  role         String
  createdAt     DateTime @default(now())
  updatedAt    DateTime @updatedAt
  meetingId    String
  meeting Meeting @relation(fields: [meetingId], references: [id], onDelete: Cascade)
  @@unique([meetingId])
  @@map("summarized_text")
}